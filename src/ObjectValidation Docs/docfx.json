{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**.csproj"
          ],
          "exclude": [
            "**/*Tests.csproj"
          ],
          "src": ".."
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
        ],
        "exclude": [
          "obj/**"
        ]
      }
    ],
    "dest": "../../docs/",
    "globalMetadataFiles": [],
    "globalMetadata": {
      "_appTitle": "ObjectValidation",
      "_appFooter": "(c) 2023 Andreas Zimmermann, wan24.de",
      //"_appLogoPath": "custom/logo.png",
      //"_appFaviconPath": "custom/favicon.ico",
      "_enableNewTab": true
    },
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}